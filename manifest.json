{
    "manifest_version":2,
    "name":"Coronavirus Immunity",
    "version":"0.1.0",
    "description":"This is a simple google chrome extension that highlight coronavirus tag",
    "icons": {
        "128":"virus.png"
    },

    "browser_action": {

        "default_popup": "popup.html"

    },
    "content_scripts": [
        {
            "matches":["<all_urls>"],
            "js": ["script.js"],
            "css": ["styles.css"]
        }
    ]
}